{
  "extends": ["web-recommended"],
  "connector": {
    "name": "local",
    "options": {
      "pattern": [
        "**",
        "!.git/**",
        "!dist/**"
      ],
      "watch": false
    }
  },
  "formatters": ["codeframe"],
  "browserslist": [
    "defaults",
    "not IE <= 11",
    "maintained node versions"
  ]
}
